encoding  utf-8
source martha.th
# Change name to the main therion file


#Surveys to output
#-----------------

export map -proj plan -layout local -o martha.pdf
#Produces plan of cave, on a single sheet

#export map -proj extended -layout local -o marthaEe.pdf

export model -fmt survex -o marthath.3d
#Survex file showing centre line only

export model -fmt kml -o Martha.kml

#export model -o martha.lox
#3d therion model to be viewed in the therion viewer

#export map -proj plan -o martha_Walls.3d
#Survex file showing walls only

layout local
  debug off
  map-header 0 0 ne
  symbol-hide group cave-centreline
endlayout
